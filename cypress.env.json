{
  "apiEndpoint": "https://the-blue-brand-2.commercelayer.co",
  "requests": [
    {
      "method": "POST",
      "url": "/oauth/token",
      "alias": "token"
    },
    {
      "method": "GET",
      "url": "/api/prices**",
      "alias": "prices"
    },
    {
      "method": "GET",
      "url": "/api/skus**",
      "alias": "skus"
    },
    {
      "method": "GET",
      "url": "/api/skus/?filter**",
      "alias": "skus"
    },
    {
      "method": "GET",
      "url": "/api/skus/**",
      "alias": "retrieveSku"
    },
    {
      "method": "POST",
      "url": "/api/orders",
      "alias": "orders"
    },
    {
      "method": "GET",
      "url": "/api/orders/**",
      "alias": "getOrders"
    },
    {
      "method": "POST",
      "url": "/api/line_items",
      "alias": "insertLineItems"
    },
    {
      "method": "GET",
      "url": "/api/line_items/**",
      "alias": "lineItems"
    },
    {
      "method": "GET",
      "url": "/api/orders/**/line_items/**",
      "alias": "retrieveLineItems"
    },
    {
      "method": "PATCH",
      "url": "/api/line_items/**",
      "alias": "updateLineItems"
    },
    {
      "method": "DELETE",
      "url": "/api/line_items/**",
      "alias": "deleteLineItems"
    },
    {
      "method": "POST",
      "url": "/api/line_item_options",
      "alias": "insertLineItemOptions"
    },
    {
      "method": "POST",
      "url": "/api/gift_cards",
      "alias": "insertGiftCard"
    },
    {
      "method": "GET",
      "url": "/api/sku_lists/**",
      "alias": "skuLists"
    }
  ],
  "pages": [
    {
      "path": "localhost:3000/order",
      "mainTitle": "JS-DROPIN-SELECT-TYPE",
      "title": "Product Page Select",
      "typeSelectSku": "select",
      "filePrefix": "sel-"
    },
    {
      "path": "localhost:3000/radioOrder",
      "mainTitle": "JS-DROPIN-RADIO-TYPE",
      "title": "Product Page Radio",
      "typeSelectSku": "radio",
      "filePrefix": "rad-"
    },
    {
      "path": "localhost:3000/categoryOrder",
      "mainTitle": "JS-DROPIN-CATEGORY-TYPE",
      "title": "Category Page",
      "typeSelectSku": "category",
      "filePrefix": "cat-"
    }
  ]
}
