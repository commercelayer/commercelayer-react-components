{
  "name": "@commercelayer/react-components",
  "version": "3.2.3",
  "description": "The Official Commerce Layer React Components",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "package.json",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      [
        "@semantic-release/github",
        {
          "addReleases": "top"
        }
      ]
    ]
  },
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "NODE_ENV=test playwright test",
    "test:e2e:coverage": "nyc npm run test:e2e && npm run coverage:report",
    "coverage:report": "nyc report --reporter=html",
    "lint": "eslint src",
    "build": "tsc -b tsconfig.prod.json --verbose",
    "postbuild": "tsc-alias -p tsconfig.prod.json &&  minimize-js lib -w -s",
    "start": "NODE_OPTIONS='--inspect' next dev",
    "version:fix": "npm version patch",
    "version:feat": "npm version minor",
    "version:perf": "npm version major",
    "publish:fix": "npm run build && npm run version:fix && git push --follow-tags && npm publish --access public",
    "publish:feat": "npm run build && npm run version:feat && git push --follow-tags && npm publish --access public",
    "publish:perf": "npm run build && npm run version:perf && git push --follow-tags && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commercelayer/commercelayer-react-components.git"
  },
  "keywords": [
    "reactjs",
    "jamstack",
    "headless",
    "ecommerce",
    "api",
    "react-components",
    "commercelayer"
  ],
  "author": {
    "name": "Alessandro Casazza",
    "email": "alessandro@commercelayer.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/commercelayer/commercelayer-react-components/issues"
  },
  "homepage": "https://github.com/commercelayer/commercelayer-react-components#readme",
  "dependencies": {
    "@ac-dev/countries-service": "^1.1.3",
    "@ac-dev/states-service": "^1.0.3",
    "@adyen/adyen-web": "^5.7.0",
    "@commercelayer/js-auth": "^2.2.9",
    "@commercelayer/sdk": "^3.5.0",
    "@stripe/react-stripe-js": "^1.7.0",
    "@stripe/stripe-js": "^1.22.0",
    "axios": "^0.25.0",
    "braintree-web": "^3.85.2",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "rapid-form": "^1.6.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@playwright/test": "^1.18.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^9.0.0",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.1",
    "@types/braintree-web": "^3.75.17",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.16",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^17.0.39",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "@zeit/next-css": "^1.0.1",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-istanbul": "^6.1.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.5.1",
    "minimize-js": "^1.2.0",
    "next": "^12.0.10",
    "nyc": "^15.1.0",
    "playwright": "^1.18.1",
    "postcss": "^8.4.6",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "tailwindcss": "^3.0.19",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "tsc-alias": "^1.5.0",
    "tsconfig-paths-jest": "^0.0.1",
    "ttypescript": "^1.5.13",
    "typescript": "^4.5.5",
    "v8-to-istanbul": "^8.1.1"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  }
}
