{
  "name": "@commercelayer/react-components",
  "version": "2.50.5",
  "description": "The Official Commerce Layer React Components",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "files": [
    "lib",
    "package.json",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      [
        "@semantic-release/github",
        {
          "addReleases": "top"
        }
      ]
    ]
  },
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "tsc -b tsconfig.prod.json tsconfig.prod.esm.json --verbose",
    "postbuild": "tsc-alias -p tsconfig.prod.json && tsc-alias -p tsconfig.prod.esm.json &&  minimize-js lib -w -s",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "start": "next start",
    "version:fix": "npm version patch",
    "version:feat": "npm version minor",
    "version:perf": "npm version major",
    "publish:fix": "npm run build && npm run version:fix && git push --follow-tags && npm publish --access public",
    "publish:feat": "npm run build && npm run version:feat && git push --follow-tags && npm publish --access public",
    "publish:perf": "npm run build && npm run version:perf && git push --follow-tags && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commercelayer/commercelayer-react-components.git"
  },
  "keywords": [
    "reactjs",
    "jamstack",
    "headless",
    "ecommerce",
    "api",
    "react-components",
    "commercelayer"
  ],
  "author": {
    "name": "Alessandro Casazza",
    "email": "alessandro@commercelayer.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/commercelayer/commercelayer-react-components/issues"
  },
  "homepage": "https://github.com/commercelayer/commercelayer-react-components#readme",
  "dependencies": {
    "@adyen/adyen-web": "^5.3.1",
    "@commercelayer/js-auth": "^2.2.6",
    "@commercelayer/sdk": "^2.3.11",
    "@stripe/react-stripe-js": "^1.6.0",
    "@stripe/stripe-js": "^1.21.2",
    "axios": "^0.24.0",
    "braintree-web": "^3.84.0",
    "countries-states-cities-service": "^1.3.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "rapid-form": "^1.6.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@playwright/test": "^1.17.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^8.0.3",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/typography": "^0.4.1",
    "@types/braintree-web": "^3.75.12",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.177",
    "@types/node": "^16.11.12",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^17.0.37",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@zeit/next-css": "^1.0.1",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.4.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-istanbul": "^6.1.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.4.3",
    "minimize-js": "^1.2.0",
    "next": "^12.0.7",
    "playwright": "^1.17.1",
    "postcss": "^8.4.4",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "tailwindcss": "^2.2.19",
    "ts-jest": "^27.1.0",
    "ts-loader": "^9.2.6",
    "tsc-alias": "^1.4.2",
    "tsconfig-paths-jest": "^0.0.1",
    "ttypescript": "^1.5.13",
    "typescript": "^4.5.2",
    "v8-to-istanbul": "^8.1.0"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  }
}
